# ProtonVPN WireGuard Configuration
WIREGUARD_PRIVATE_KEY=your_private_key_here
SERVER_COUNTRIES="United Kingdom"
SERVER_CITIES="London"

# User & Timezone Settings
PUID=1000
PGID=1000
TZ=Europe/London

# Gluetun API Authentication (Secures VPN Port Sync)
GLUETUN_USER=your_admin_username (make up)
GLUETUN_PASS=your_admin_password (make up)

# qBittorrent Port Forwarding Sync
GSP_GTN_API_KEY=your_random_api_key_here (docker run --rm qmcgaw/gluetun genkey)
GSP_QBITTORRENT_PORT=your_forwarded_port_here (leave glueton will forward after it runs)
